<h1>Editing exercise_group</h1>

<% form_for(@exercise_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :number %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label :lecture_time %><br />
    <%= f.text_field :lecture_time %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @exercise_group %> |
<%= link_to 'Back', exercise_groups_path %>